// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model manageUser {
  id                 Int     @id @default(autoincrement())
  first_name         String
  last_name          String
  username           String  @unique
  avatar             String
}